<h1>Movies#new</h1>
<p>Find me in app/views/movies/new.html.erb</p>

<%= form_with(model: @movie) do |f| %>

    <h4>Title</h4>
    <%= f.text_field :name %>

    <h4>Body</h4>
    <%= f.text_area :body %>

    <h4>image</h4>
    <%= f.attachment_field :image %>

    <%= f.submit 'Create movie' %>

<% end %>